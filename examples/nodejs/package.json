{
  "name": "joblet-nodejs-examples",
  "version": "1.0.0",
  "description": "Node.js examples for Joblet job execution platform",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "node app.js",
    "dev": "nodemon app.js",
    "lint": "eslint . --ext .js --format json --output-file /volumes/nodejs-projects/reports/eslint.json || eslint . --ext .js",
    "audit": "npm audit --json > /volumes/nodejs-projects/reports/audit.json || npm audit",
    "test:unit": "jest --testPathPattern=__tests__ --json --outputFile=/volumes/nodejs-projects/reports/jest.json || jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration/ --runInBand",
    "build": "webpack --mode=production --stats-json --stats-file=/volumes/nodejs-projects/reports/webpack-stats.json || webpack --mode=production",
    "process-data": "node process_data.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.4.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "axios": "^1.4.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "eslint": "^8.44.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "keywords": ["joblet", "nodejs", "microservices", "api", "testing"],
  "author": "Joblet Examples",
  "license": "MIT"
}